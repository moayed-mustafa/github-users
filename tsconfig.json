{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react",
    "target": "es5",
    "explainFiles": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "paths": {
      "~*": [
        "./*"
      ],
      "/*": [
        "./*"
      ],
      "@/components*": [
        "./src/components/*"
      ],
      "@/context*": [
        "./src/context/*"
      ],
      "@/models*": [
        "./src/models/*"
      ],
      "@/api*": [
        "./src/api/*"
      ],
    }
  }
}
